{
  "name": "vanilla-oven",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "REACT_NATIVE_PACKAGER_HOSTNAME=$(ifconfig en0 | grep 'inet ' | awk '{print $2}') expo start",
    "start_clear": "REACT_NATIVE_PACKAGER_HOSTNAME=$(ifconfig en0 | grep 'inet ' | awk '{print $2}') expo start --clear",
    "android": "REACT_NATIVE_PACKAGER_HOSTNAME=$(ifconfig en0 | grep 'inet ' | awk '{print $2}') expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "apk": "eas build -p android --profile preview --local",
    "apk-remote": "eas build -p android --profile build-remote-apk"
  },
  "dependencies": {
    "@babel/core": "^7.20.0",
    "@expo/vector-icons": "latest",
